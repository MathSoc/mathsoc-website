div(id="footer")
    div(class="section")
        b PHONE NUMBER 
        br
        p #{footer.phoneNumber}
    div(class="section")
        strong MATHSOC OFFICE 
        br
        p #{footer.addressLine1}
        p #{footer.addressLine2}
        p #{footer.addressLine3}
    div(class="section blank")
    div(class="section social-section")
        b(class="social-header") GET SOCIAL
        br
        div(class="socials")
            a(href="" + footer.socialLinks.facebook + "" target="_blank")
                img(src="/assets/img/social/facebook.png" alt="Go to MathSoc's Facebook page")
            a(href="" + footer.socialLinks.instagram + "" target="_blank")
                img(src="/assets/img/social/instagram.png" alt="Go to MathSoc's Instagram page")
            a(href="" + footer.socialLinks.mail + "" target="_blank")
                img(src="/assets/img/social/mail.png" alt="Email MathSoc")
            a(href="" + footer.socialLinks.discord + "" target="_blank")
                img(src="/assets/img/social/discord.svg" style="filter: invert(1)" 
                    alt="Learn more about accessing MathSoc's Discord server")